package jkl;
import java.util.Scanner;
public class ll {
	public static void main(String[] args) {
	Scanner input = new Scanner(System.in);
	System.out.println("Enter the integers between 1 and 100: ");
	int[] a=new int[110];
	int k=1,i;
	for(i=1;i<=100;i++){
		a[i]=0;
	}
	while(k!=0){
		k=input.nextInt();
		a[k]=a[k]+1;
	}
	for(i=1;i<=100;i++){
		if(a[i]!=0){
			System.out.println(i+" occurs " +a[i]+" times");
		}
	}
	}
}

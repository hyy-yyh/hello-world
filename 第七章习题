package ww;
import java.util.Scanner;
import java.util.Arrays;
public class bubblesort {

	public static void main(String[] args){		
		boolean[] box = new boolean[101];	
		int flag = 0;		
		for(int i = 1; i <= 100; ++i){		
			if(i == 1){		
				for(int j = 1; j <= 100; ++j)		
					box[j] = !box[j];			
				}	
			else if(i == 2){	
				for(int j = i; j <= 100; j += 2){		
					box[j] = !box[j];			
					}		
				}	
			else{		
				for(int j = i; j <= 100; j += i){	
					box[j] = !box[j];			
					}	
				}		
			}			
		for(int i = 1; i <= 100; ++i){		
			if(box[i])			
				System.out.println(i);	
			}	
		}
	}
